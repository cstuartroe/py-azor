fn:INT(a:INT, b:INT, c:INT) = (c + (a * b))

all:BOOL(l:[BOOL]) = ( if (l -> head ~ tail)
                       then (if head then all(tail) else false)
                       else true )

any:BOOL(l:[BOOL]) = ( if (l -> head ~ tail)
                       then (if head then true else any(tail))
                       else false )

fib:INT(n:INT) = l with (fibHelper(n) -> (l, r))

tup:(INT, INT) = (2, 50)

fibHelper:(INT, INT) (n:INT) = ( if (n == 0)
						         then (0, 1)
							     else (printi(r), (l + r)) with (fibHelper((n - 1)) -> (l, r)) )

fib50:INT = fib(b) with (tup -> (a, b))

range:[INT](n:INT) = ( if (n < 0)
                       then []
                       else n ~ range((n - 1)) )

print_list:[INT](l:[INT]) = ( if (l -> first ~ rest)
                              then (printi(first) ~ print_list(rest))
                              else [] )

zip:[(INT, INT)](l1:[INT], l2:[INT]) = ( if (l1 -> first1 ~ rest1)
                                         then ( if (l2 -> first2 ~ rest2)
                                                then (first1, first2) ~ zip(rest1, rest2)
                                                else [] )
                                         else [] )

main:BOOL() = (if (print_list(range(10)) -> head ~ tail) then true else false)
